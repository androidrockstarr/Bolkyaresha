<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rajpriya.com.bolkyaresha" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="rajpriya.com.bolkyaresha.App"
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="rajpriya.com.bolkyaresha.MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.facebook.LoginActivity" >
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name="rajpriya.com.bolkyaresha.BrowserActivity"
            android:screenOrientation="portrait"
            android:label="" >
        </activity>
        <activity
            android:name="rajpriya.com.bolkyaresha.JokeDetailsActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_joke_details" >
        </activity>
    </application>

</manifest>
