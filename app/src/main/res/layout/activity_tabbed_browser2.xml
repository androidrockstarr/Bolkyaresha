<?xml version="1.0" encoding="utf-8"?>
<com.apps.bolkyareshafree.CustomSwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/swipe_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<RelativeLayout
    android:id="@+id/grid"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <android.support.v4.view.ViewPager
        android:id="@+id/pager"
        android:layout_above="@+id/adView"
        android:layout_width="match_parent" android:layout_height="match_parent"
        tools:context="com.apps.bolkyareshafree.TabbedBrowserActivity" />

</RelativeLayout>


</com.apps.bolkyareshafree.CustomSwipeRefreshLayout>


